class Bob {
    let @@bobs of Int32;
    def _(name of String) {
        @@bobs = @@bobs + 1;
        let @name = name;
    }
    def greet() {
        print @"Hello, #{@name}!";
    }
}

class Bob {
    def _(age of Int32) {
        let @age = age;
    }
    def greet(msg of String) {
        print @"#{msg}, #{@name}, #{@age}!";
    }
}

# Until the standard library is added, this is required for test calls to be valid
class Int32 { def +(other of Int32) of Int32 {} }
def print(msg of String) {}
